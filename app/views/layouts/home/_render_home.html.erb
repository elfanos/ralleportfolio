                                  clearInterval(webONE);
                                  return startChangeTextEffect(node, index+1, false);
                              }
                          }
                      } else {
                          console.log("one time plz");
                          clearInterval(webONE);
                          if(node._intervalTime === 0) {
                              addMoreLetters(node,node._textView.engineering,(index));
                              node._intervalTime = 1;
