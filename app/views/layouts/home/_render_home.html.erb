          function playAnimations() {
              _tileTimelineHoverIn.pause();
              _tileTimelineHoverOut.restart();
          }
      }
      var imageGirdsHome = [];
      function initializeProjectsHome() {
          for(var i = 0; i < 2; i++){
              (function (i) {
                  setTimeout(function () {
                      var item = new ProjectListItem(
                          projectPictureUrl[i],
                          projectID[i],
                          2,
                          2,
                          0,
                          1.5
                      );
                      imageGirdsHome.push(new ImageGridHome(item));

                  },50);
              })(i);
          }
      }

      homeScreenAnimiation();
      $(projectID[0]).hide();
      $(projectID[1]).hide();
      $(projectID[2]).hide();
      setTimeout(function () {
          initializeProjectsHome();
      },1500);
      /**
       * text animation interval
       * */
      setTimeout(function () {
        animationWelcome.welcome.showDivs();
        animationWelcome.toLetter.showDivs();
        animationWelcome.myLetter.showDivs();
        animationWelcome.siteLetter.showDivs();

      },500);
      setTimeout(function () {
          animateRighText.ux.showDivs();
          textUXEffect._increaseMargin();

          textCodingEffect._increaseMargin();
          animateRighText.coding.showDivs();

          textAndEffect._increaseMargin();
          animateRighText.and.showDivs();

          textIntervalEffect._increaseMargin();
          animateRighText.interval.showDivs();
      },1000);
      setTimeout(function () {
          textUXEffect._releaseMargin();
          textCodingEffect._releaseMargin();
          textAndEffect._releaseMargin();
          textIntervalEffect._releaseMargin();
          $(projectID[0]).fadeIn('fast');
      },1500);
      setInterval(function () {
          beginTextChangeAnimation(animationIntervalText.webOne, 1, false);
      },6000);
      pictureAnimation();
      function pictureAnimation() {
          (function (position) {
              setInterval(function () {
                  if(position === 0) {
                      imageGirdsHome[0]._stopAni();
                      setTimeout(function () {
                          imageGirdsHome[0]._startAni();
                      },500);
                  }
              },6000);
          })(position)
      }
  });
</script>

