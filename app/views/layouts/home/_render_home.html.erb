      function TextAnimationHome(context,letters) {
          this.context = context;
          this.letters = letters;
          this.interval = [];
          this.textDivTags = [];
          this._intervalTime = 0;
          this._textView = {
              web: ['w', 'e', 'b'],
              engineering: ['e','n','g','i','n','e','e','r','i','n','g'],
              interaction: ['i', 'n', 't','e','r','a','c','t','i','o','n']
          };
          this._animationSetted = false;
          this.removeLetters = function () {
              while(this.context.firstChild){
                  this.context.removeChild(this.context.firstChild);
              }
          };
          this.initializeLetters = function() {
              this.context.style.width = $(window).height() + "px";
              for(var i = 0; i < this.letters.length; i++){
                  var div = document.createElement("div");
                  div.className = "char chars"+i;
                  div.id = "id-chars"+i;
                  var innerText = document.createElement("div");
                  innerText.className = "char-inner";
                  innerText.innerHTML = letters[i];
                  console.log("innertexT?", letters[i]);
                  div.appendChild(innerText);
                  div.style.visibility = "hidden";
                  //parentDiv.querySelector("#child2") != null;
                  //this._checkIfIfCharAlreadyExist(this.context.childNodes)
                  var hasChild = this.context.querySelector(div.id) != null;
                  this.context.appendChild(div);
              }
          };
          this._addANewLetter = function (letter) {
              var div = document.createElement("div");
              div.className = "char chars1";
              div.id = "id-chars";
              var innerText = document.createElement("div");
              innerText.className = "char-inner";
              innerText.innerHTML = letter;
              div.appendChild(innerText);
              var hasChild = this.context.querySelector(div.id) != null;
              this.context.appendChild(div);
          };
          this._removeLetter = function (index) {
              var childs = this.context.childNodes;
              while(Object.size(childs) > 4){
                  console.log(this.context.lastChild.innerHTML);
                  this.context.removeChild(this.context.lastChild);
              }
          };
          this.initializeTags = function () {
              var childs = this.context.childNodes;
              for(var i = 0; i < childs.length; i++){
                  this.textDivTags.push(childs[i].childNodes[0]);
              }
              this._animationSetted = true;
          };
          this.changeTextOnChild = function (index,letter){
              var childs = this.context.childNodes;
              for(var i = 0; i < loopLength(childs); i++){
                 if(i === index){
                     if(letter != undefined) {
                         childs[i].innerHTML = letter;
                     }
                 }
              }
          };
          this.getLength = function () {
            return Object.size(this.context.childNodes);
          };
          this.showDivs = function () {
              var childs = this.context.childNodes;
              for(var i = 1; i < loopLength(childs); i++){
                  (function (node, j) {
                      setTimeout(function () {
                       //   console.log("wellwell", childs[i]);
                          node.style.visibility = "visible";
                      },50*j);
                  })(childs[i], i)
              }
          };
      }
      function getRandomizedLetter() {
          var letters = "abcdeinoqr";
          function getLetterFromString(index) {
              for(var i = 0; i < letters.length; i++){
                  if(i === index){ return letters.charAt(index); }
              }
          }
          return getLetterFromString(Math.floor(Math.random() * letters.length) + 1);
