      /**
       * check if more or less div elements need to be added
       * also change the char to the new one in the letter array
       * */
      function addMoreLetters(node,letters,index){
          var numOfMoreLetters = letters.length - index;
          if(numOfMoreLetters >= 0) {
              for (var i = 0; i < index; i++) {
                  node.changeTextOnChild((i+1), letters[i]);
              }
              for (var j = index; j < (numOfMoreLetters + index); j++) {
                  node._addANewLetter(letters[j]);
              }
          }
          if(numOfMoreLetters < 0){
            for (var c = 0; c < (letters.length); c++) {
                node.changeTextOnChild((c+1), letters[c]);
            }
            setTimeout(function () {
                node._removeLetter(letters.length);
            },20);
          }

      }
