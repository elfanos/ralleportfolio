    function HoverEffectOnItemSelect(textAnimation, context, interval, style){
        this._textAnimation = textAnimation;
        this._context = context;
        this._instance = null;
        this._hoverInterval = interval;
        this._speedAnimation = 0.1;
        this._style = style;
        this._middleElement = null;
        this._dontHoverBack = false;
        this._delayAnimation = function (child,width, i) {
            var id = i;
            (function (instance) {
                instance._hoverInterval.push(setInterval(function () {
                    if(instance._style == "project" || instance._style == "music") {
                        if (width < ($(window).width() / instance._textAnimation.letters.length) - 50) {
                            width += 5;
                            child.style.width = width + "px";
                            child.style.letterSpacing = 0;
                        } else {
                            clearInterval(instance._hoverInterval[id]);
                        }
                    }else if(instance._style == "about" || instance._style == "contact") {
                        if (width < ($(window).height() / instance._textAnimation.letters.length) - 20) {
                            width += 5;
                            child.style.width = width + "px";
                            child.style.letterSpacing = 0;
                        } else {
                            clearInterval(instance._hoverInterval[id]);
                        }
                    }
                  }, 1)
                );
            })(this._instance);
        };
        this._hoverEffectOn = function (style) {
            console.log("yaras?2");
            this._hovered = true;
            if(this._style == "project" || this._style == "music"){
              this._context.style.width = "100%";
            }
            if(this._hoverInterval.length == 0 && style == null) {
                (function (instance) {
                    setTimeout(function () {
                        for (var i = 0; i < instance._context.childNodes.length; i++) {
                            instance._lowerLag(i,0,i);
                        }
                    }, 20);

                })(this._instance);
            }else{
                if(style != null) {
                    if (style == "pagingEffect") {
                        this._dontHoverBack = true;
                        console.log("bajs");
                        (function (instance) {
                            setTimeout(function () {
                                for (var i = 0; i < instance._context.childNodes.length; i++) {
                                    if(instance._style == "about" || instance._style == "contact") {
                                        instance._context.childNodes[i].style.width = ($(window).height() / instance._textAnimation.letters.length) - 20 + "px";
                                    }else if(instance._style == "project" || instance._style == "music"){
                                        console.log("project??");
                                        instance._context.childNodes[i].style.width = ($(window).width() / instance._textAnimation.letters.length) - 5 + "px";
                                    }
                                }
                            }, 20);

                        })(this._instance);
                    }
                }
            }
        };
        this._lowerLag = function (child, width, i) {
            (function (instance) {
                setTimeout(function () {
                    instance._delayAnimation(instance._context.children[child], 0, child)
                },10*child)
            })(this._instance)

        };
        this._clearIntervals = function(hover){
            for(var i = 0; i < hover.length; i++){

                clearInterval(hover[i]);

            }
            this._instance._hoverInterval = [];
        };
        this._hoverEffectOut = function () {
            console.log("hover out");
              for (var i = 0; i < this._context.childNodes.length; i++) {
                  if (this._style == "project" || this._style == "music") {
                      this._noLagAnimation(
                          i,
                          ($(window).width() / this._textAnimation.letters.length) - 50,
                          i
                      );
                  } else if (this._style == "about" || this._style == "contact") {
              //        console.log("hover out 2");
                      this._noLagAnimation(
                          i,
                          ($(window).height() / this._textAnimation.letters.length) - 20,
                          i
                      );
                     // this._setRightWidth(i,i);
                      if(i == this._context.childNodes.length){
                          //this._clearIntervals(this._hoverInterval);
                          (function (instance) {
                              setTimeout(function () {
                                  instance._hovered = false;
                              }, 100)
                          })(this._instance);
                      }
                  }
              }

        };
        this._setRightWidth = function (child,i) {
           // console.log("extifunction??");
            (function (instance) {

                    instance._noLagAnimation(
                        i,
                        instance._context.children[child].clientWidth,
                        i
                    );

            })(this._instance);
        };
        this._noLagAnimation = function (child,width, i) {
            //console.log("hover out 3");
            (function (instance) {
                setTimeout(function () {
                    instance._delayOutAnimation(instance._context.children[child], width, child)
                },10*child)
            })(this._instance)
        };
        this._delayOutAnimation = function (child,width, i) {
            var id = i;
            (function (instance) {
                var test  = setInterval(function () {
                    if(instance._style == "project" || instance._style == "music") {
                        if (width > 22)  {
                            width -= 5;
                            child.style.width = width + "px";
                        } else {
                            clearInterval(test);
                        }
                    }else if(instance._style == "about" || instance._style == "contact"){
                        if (width > 22) {
                            //console.log("hover out 5", width);
                            width -= 5;
                            child.style.width = width + "px";
                        } else {
                            clearInterval(test);
                        }
                    }
                    }, 10);
            })(this._instance);
        };
    }

    function TextAnimation(context,letters) {
        this.context = context;
        this.letters = letters;
        this.interval = [];
        this.textDivTags = [];
        this._animationSetted = false;
        this.removeLetters = function () {
            while(this.context.firstChild){
                this.context.removeChild(this.context.firstChild);
            }
        };
        this.initializeLetters = function() {
            this.context.style.width = $(window).height() + "px";
            for(var i = 0; i < this.letters.length; i++){
                var div = document.createElement("div");
                div.className = "char chars"+i;
                div.id = "id-chars"+i;
                var innerText = document.createElement("div");
                innerText.className = "char-inner";
                innerText.innerHTML = letters[i];
                div.appendChild(innerText);
                //parentDiv.querySelector("#child2") != null;
                //this._checkIfIfCharAlreadyExist(this.context.childNodes)
                var hasChild = this.context.querySelector(div.id) != null;

                    this.context.appendChild(div);

            }
        };
        this.initializeTags = function () {
            var childs = this.context.childNodes;
            for(var i = 0; i < Object.size(childs); i++){
                this.textDivTags.push(childs[i].childNodes[0]);
            }
            this._animationSetted = true;
        }
        this._checkIfIfCharAlreadyExist = function (childNodes) {
            console.log(childNodes.length);
            for(var i = 0; i < childNodes.length; i++) {
                console.log(childNodes[i].id);
                if (childNodes[i].id == "id-chars"+i) {
                    this._animationSetted = true;
                } else {
                    console.log("in else duplicates");
                    this._animationSetted = false;
                }
            }

        };
    }

