
<%logger.info "in lag"%>
$('html,body').scrollTop(0);
$("#content-container").animate({ scrollTop: 0 }, 'fast');

$("#projectPage").fadeOut("slow");
$("#navProjects").fadeOut("slow");
setTimeout(function () {
    history.pushState('lag', document.title, '/' + 'projects' + '/lag');
    //Do some cool animation
    $("#projectPage").
    html(
        "<%= escape_javascript(render partial: 'layouts/projects/info/render_google_glass',locals: { state: "googleglass" }) %>"
    ).fadeIn("slow");

    localStorage.infoState = "lag";
}, 2000);